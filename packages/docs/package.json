{
  "private": false,
  "name": "@svelte-mui/docs",
  "version": "0.0.1-beta1",
  "description": "Svelte Material Design Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/boomfly/svelte-mui.git"
  },
  "homepage": "https://svelte-mui.herokuapp.com",
  "bugs": {
    "url": "https://github.com/boomfly/svelte-mui/issues",
    "email": "boomfly@rambler.ru"
  },
  "keywords": [],
  "author": "Ivan Chalyk <boomfly@rambler.ru>",
  "license": "MIT",
  "main": "dist/node/index.js",
  "browser": "dist/browser/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run docs:build",
    "watch": "concurrently -n 'compile-server,compile-client,runtime' 'npm:docs:compile-server' 'npm:docs:compile-client' 'npm:docs:server'",
    "docs:clean": "rm -rf dist/*",
    "docs:build": "npm run docs:clean && npm run docs:build-client && npm run docs:build-server",
    "docs:server": "DEBUG=svelte-mui:* DEBUG_COLORS=true nodemon --ignore db/ --ignore .cache/ --ignore dist/public/ dist/index.js",
    "docs:compile-server": "TARGET=node parcel watch -t node --no-cache --out-dir dist/ src/index.coffee",
    "docs:compile-client": "parcel watch --out-dir dist/public src/client/index.html",
    "docs:build-client": "parcel build --no-cache --out-dir dist/public src/client/index.html",
    "docs:build-server": "TARGET=node parcel build -t node --no-cache --out-dir dist src/index.coffee",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-json": "^4.1.0",
    "babel-loader": "^8.1.0",
    "coffee-loader": "^1.0.1",
    "coffeescript": "^2.5.1",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "mini-css-extract-plugin": "^1.2.1",
    "nodemon": "^2.0.6",
    "parcel": "^1.12.4",
    "parcel-plugin-svelte": "^4.0.6",
    "raw-loader": "^4.0.2",
    "rollup-plugin-coffee-script": "^2.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^6.1.0",
    "style-loader": "^2.0.0",
    "svelte": "^3.29.4",
    "svelte-check": "^1.0.36",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.2.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@feathersjs/authentication": "^4.5.9",
    "@feathersjs/authentication-client": "^4.5.9",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^4.5.9",
    "@feathersjs/client": "^4.5.9",
    "@feathersjs/configuration": "^4.5.9",
    "@feathersjs/errors": "^4.5.9",
    "@feathersjs/express": "^4.5.9",
    "@feathersjs/feathers": "^4.5.9",
    "@feathersjs/socketio": "^4.5.9",
    "@feathersjs/socketio-client": "^4.5.9",
    "@svelte-mui/core": "0.0.1-beta1",
    "bufferutil": "^4.0.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "feathers-nedb": "^5.1.0",
    "handlebars": "^4.7.6",
    "helmet": "^3.15.0",
    "lowdb": "^1.0.0",
    "nedb": "^1.8.0",
    "rollup": "^2.32.1",
    "serve-favicon": "^2.5.0",
    "source-map-support": "^0.5.19",
    "utf-8-validate": "^5.0.3",
    "webpack-node-externals": "^2.5.2"
  }
}
