{
  "private": true,
  "name": "root",
  "version": "0.0.1-beta1",
  "description": "Svelte Material Design Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/boomfly/svelte-mui.git"
  },
  "homepage": "https://svelte-mui.herokuapp.com",
  "bugs": {
    "url": "https://github.com/boomfly/svelte-mui/issues",
